		<function name="fntbltituloaiauad" id="Function4846929" isSystem="false" params_known="n" >
			<string><![CDATA[CREATE OR REPLACE FUNCTION mgsis.fntbltituloaiauad()
 RETURNS trigger
 LANGUAGE plpgsql
AS $function$
				DECLARE
				vCodTituloAgrupamento bigint;
				vDebitoTotal numeric (14,2);
				vCreditoTotal numeric (14,2);
				BEGIN
				IF (TG_OP = 'DELETE') THEN
					vCodTituloAgrupamento = OLD.codTituloAgrupamento;
				ELSE
					vCodTituloAgrupamento = NEW.codTituloAgrupamento;
				END IF;
				IF vCodTituloAgrupamento IS NOT NULL THEN
					update tbltituloagrupamento
					set 
						debito = q.debito,
						credito = q.credito
					from (
						select 
							tbltitulo.codtituloagrupamento, 
							sum(coalesce(tbltitulo.debito, 0)) as debito, 
							sum(coalesce(tbltitulo.credito, 0)) as credito
						from tbltitulo
						where tbltitulo.codtituloagrupamento = vCodTituloAgrupamento
						group by tbltitulo.codtituloagrupamento
						) q
					where tbltituloagrupamento.codtituloagrupamento = vCodTituloAgrupamento
					AND tbltituloagrupamento.codtituloagrupamento = q.codtituloagrupamento;
				END IF;
				RETURN NEW;
				END;
			$function$
 ;]]></string>
		</function>
		<function name="fntbltituloau" id="Function4846929" isSystem="false" params_known="n" >
			<string><![CDATA[CREATE OR REPLACE FUNCTION mgsis.fntbltituloau()
 RETURNS trigger
 LANGUAGE plpgsql
AS $function$
DECLARE
	DebitoMov numeric(14,2);
	CreditoMov numeric(14,2);
BEGIN
	IF current_user <> 'mgsis_replicacao' AND current_user <> 'mgsis_yii' THEN
		IF COALESCE(NEW.DEBITO, 0) > COALESCE(OLD.DEBITO, 0) THEN
			DebitoMov = COALESCE(NEW.DEBITO, 0) - COALESCE(OLD.DEBITO, 0);
		END IF;
		IF COALESCE(NEW.DEBITO, 0) < COALESCE(OLD.DEBITO, 0) THEN
			CreditoMov = COALESCE(OLD.DEBITO, 0) - COALESCE(NEW.DEBITO, 0);
		END IF;
		IF COALESCE(NEW.Credito, 0) > COALESCE(OLD.Credito, 0) THEN
			CreditoMov = COALESCE(NEW.Credito, 0) - COALESCE(OLD.Credito, 0);
		END IF;
		IF COALESCE(NEW.Credito, 0) < COALESCE(OLD.Credito, 0) THEN
			DebitoMov = COALESCE(OLD.Credito, 0) - COALESCE(NEW.Credito, 0);
		END IF;
		IF COALESCE(DebitoMov, 0) <> 0 OR COALESCE(CreditoMov, 0) <> 0 THEN
			INSERT INTO tblMovimentoTitulo
				( codTipoMovimentoTitulo 
				, codTitulo 
				, codPortador 
				, codTituloRelacionado
				, Debito
				, Credito 
				, Historico 
				, Transacao 
				, Sistema 		
				)
			SELECT 
				  200
				, NEW.codTitulo
				, NEW.codPortador
				, null
				, DebitoMov
				, CreditoMov
				, null
				, NOW()
				, NOW()
			FROM tblTipoTitulo 
			WHERE tblTipoTitulo.codTipoTitulo = NEW.codTipoTitulo;
		END IF;
	END IF;
	RETURN NEW;
END;
$function$
;]]></string>
		</function>
		<function name="geraauditoria" id="Function4846929" isSystem="false" params_known="n" >
			<string><![CDATA[CREATE OR REPLACE FUNCTION mgsis.geraauditoria()
 RETURNS trigger
 LANGUAGE plpgsql
AS $function$
DECLARE
	--codigo
	tcolunacodigo text;
	tcodantigo bigint;
	tcodnovo bigint;
	--coluna alterada
	tcolunaalterada TEXT;
	tantigo TEXT;
	tnovo TEXT;
BEGIN
	IF current_user <> 'mgsis_replicacao' THEN
		--Pega o campo da chave primaria
		SELECT min(column_name) INTO tcolunacodigo 
		  FROM information_schema.key_column_usage 
		 WHERE TABLE_SCHEMA = TG_TABLE_SCHEMA
		   AND TABLE_NAME = TG_TABLE_NAME  
		   AND constraint_name like '%pkey';
		--Pega o Valor antigo da chave primaria
		IF TG_OP IN ('UPDATE', 'DELETE') THEN
			EXECUTE 'SELECT ($1).' || tcolunacodigo || '::text' INTO tcodantigo USING OLD;
		END IF;
		--Pega o Valor novo da chave primaria
		IF TG_OP IN ('INSERT', 'UPDATE') THEN
			EXECUTE 'SELECT ($1).' || tcolunacodigo || '::text' INTO tcodnovo USING NEW;
		END IF;
		--se for delete ou insert grava na tabela de auditoria
		IF TG_OP IN ('INSERT', 'DELETE') THEN
			INSERT INTO tblAuditoria (tabela       , colunacodigo , codantigo , codnovo , colunaalterada , antigo , novo , operacao              )
					  VALUES (TG_TABLE_NAME, tcolunacodigo, tcodantigo, tcodnovo, tcolunaalterada, tantigo, tnovo, SUBSTRING(TG_OP, 1, 1));
		--se for update percorre campos da tabela, e tem campo alterado
		ELSIF NEW IS DISTINCT FROM OLD THEN
			--se foi alterado o codigo da chave primaria
			--altera o codigo na tabela de auditoria
			IF tcodnovo <> tcodantigo THEN
				UPDATE tblAuditoria 
				   SET codnovo = tcodnovo
				 WHERE tabela = TG_TABLE_NAME
				   AND colunacodigo = tcolunacodigo
				   AND codantigo = tcodantigo;
				UPDATE tblAuditoria 
				   SET codnovo = tcodnovo
				 WHERE tabela = TG_TABLE_NAME
				   AND colunacodigo = tcolunacodigo
				   AND codnovo = tcodantigo;
			END IF;
			--percorre campos da tabela
			FOR tcolunaalterada IN SELECT column_name 
						FROM information_schema.columns 
						WHERE table_schema = TG_TABLE_SCHEMA
						AND table_name = TG_TABLE_NAME
						AND column_name <> tcolunacodigo
			LOOP	
				--soh entra se a coluna da tabela nao estiver na lista de excecoes 
				IF (SELECT COUNT(codAuditoriaExcecao) FROM tblAuditoriaExcecao WHERE tabela = TG_TABLE_NAME AND campo = tcolunaalterada) = 0 THEN
					--busca valor antigo do campo
					EXECUTE 'SELECT ($1).' || tcolunaalterada || '::text' INTO tantigo USING OLD;
					--busca valor novo do campo
					EXECUTE 'SELECT ($1).' || tcolunaalterada || '::text' INTO tnovo   USING NEW;
					IF COALESCE(tantigo, '') <> COALESCE(tnovo, '') THEN
						INSERT INTO tblAuditoria (tabela       , colunacodigo , codantigo , codnovo , colunaalterada , antigo , novo , operacao              )
								  VALUES (TG_TABLE_NAME, tcolunacodigo, tcodantigo, tcodnovo, tcolunaalterada, tantigo, tnovo, SUBSTRING(TG_OP, 1, 1));
					END IF;
				END IF;
			END LOOP;
		END IF;
	END IF;
	RETURN NULL;
END;
$function$
 ;]]></string>
		</function>
