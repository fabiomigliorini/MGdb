		</materialized_view>
		<materialized_view name="mwvendas" >
			<view_script><![CDATA[CREATE MATERIALIZED VIEW ${materializedView} AS  SELECT n.codnegocio,
    (n.lancamento)::date AS dia,
    n.codfilial,
    f.filial,
    npb.codprodutobarra,
    pb.barras,
    p.codproduto,
    p.produto,
    n.codpessoavendedor,
    v.fantasia AS vendedor,
    nat.codnaturezaoperacao,
    nat.naturezaoperacao,
    npb.quantidade,
    ((COALESCE(npb.valorprodutos, (0)::numeric) - COALESCE(npb.valordesconto, (0)::numeric)) * (
        CASE
            WHEN (nat.codoperacao = 1) THEN '-1'::integer
            ELSE 1
        END)::numeric) AS valorvenda,
    p.comissao AS comissaoproduto,
    COALESCE((v.comissao)::integer, 1) AS comissaovendedor
   FROM ((((((tblnegocio n
     JOIN tblfilial f ON ((f.codfilial = n.codfilial)))
     JOIN tblnegocioprodutobarra npb ON ((npb.codnegocio = n.codnegocio)))
     JOIN tblprodutobarra pb ON ((pb.codprodutobarra = npb.codprodutobarra)))
     JOIN tblproduto p ON ((p.codproduto = pb.codproduto)))
     JOIN tblnaturezaoperacao nat ON (((nat.codnaturezaoperacao = n.codnaturezaoperacao) AND (nat.venda OR nat.vendadevolucao))))
     LEFT JOIN tblpessoa v ON ((v.codpessoa = n.codpessoavendedor)))
  WHERE ((n.codnegociostatus = 2) AND (f.codempresa = 1) AND (npb.inativo IS NULL))]]></view_script>
