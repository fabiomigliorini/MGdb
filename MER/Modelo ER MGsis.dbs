                CASE
                    WHEN (sum(em.saldoquantidade) <> (0)::numeric) THEN sum(em.saldoquantidade)
                    ELSE NULL::numeric
                END) AS customedio
           FROM (((((tblestoquelocalprodutovariacao elpv
             JOIN tblprodutovariacao pv ON ((pv.codprodutovariacao = elpv.codprodutovariacao)))
             JOIN tblestoquelocal el ON ((el.codestoquelocal = elpv.codestoquelocal)))
             JOIN tblfilial f ON ((f.codfilial = el.codfilial)))
             JOIN tblestoquesaldo es ON (((es.codestoquelocalprodutovariacao = elpv.codestoquelocalprodutovariacao) AND (es.fiscal = true))))
             JOIN tblestoquemes em ON ((em.codestoquemes = ( SELECT em2.codestoquemes
                   FROM tblestoquemes em2
                  WHERE ((em2.codestoquesaldo = es.codestoquesaldo) AND (em2.mes <= '2024-12-31'::date))
                  ORDER BY em2.mes DESC
                 LIMIT 1))))
          WHERE (f.codfilial = ANY (ARRAY[(101)::bigint, (102)::bigint, (103)::bigint, (104)::bigint, (105)::bigint]))
          GROUP BY el.codfilial, pv.codproduto) fiscal ON ((fiscal.codproduto = p.codproduto)))
     LEFT JOIN tblncm n ON ((n.codncm = p.codncm)))
  WHERE ((p.codtipoproduto = 0) AND (fiscal.saldoquantidade <> (0)::numeric))
  ORDER BY p.produto, p.codproduto, fiscal.codfilial]]></view_script>
