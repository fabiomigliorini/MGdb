		<function name="criatriggers_geraauditoria" id="Function4846928" isSystem="false" params_known="n" >
			<string><![CDATA[CREATE OR REPLACE FUNCTION mgsis.criatriggers_geraauditoria(brecria boolean DEFAULT false)
 RETURNS boolean
 LANGUAGE plpgsql
AS $function$
DECLARE
	rTabelas RECORD;
BEGIN
	--percorre as tabelas do banco de dados
	FOR rTabelas IN 
		select tablename, tgname
		from pg_tables
		left join pg_trigger on (pg_trigger.tgname = pg_tables.tablename || '_trigger_geraauditoria')
		where pg_tables.tablename not in ('tblauditoria', 'tblbaseremota', 'tblauditoriatransmissao', 'tblcodigo')
		AND pg_tables.schemaname = 'mgsis'
	LOOP	
	
		--se jah tem a trigger, apaga
		IF rTabelas.tgname IS NOT NULL AND brecria = true THEN
			RAISE NOTICE E'Apagando Trigger %', rtabelas.tgname;
			EXECUTE 'DROP TRIGGER ' || rtabelas.tgname || ' ON ' || rTabelas.tablename;
		END IF;
		
		--cria nova trigger
		IF rTabelas.tgname IS NULL OR brecria = true THEN
			RAISE NOTICE E'Criando Trigger %', rTabelas.tablename || '_trigger_geraauditoria';
			EXECUTE 'CREATE TRIGGER ' || rtabelas.tablename || '_trigger_geraauditoria AFTER INSERT OR UPDATE OR DELETE ON ' || rtabelas.tablename || ' FOR EACH ROW EXECUTE PROCEDURE GeraAuditoria()' ;
		end if;
		
	END LOOP;
	
	RETURN TRUE;
END;
$function$
;]]></string>
		</function>
