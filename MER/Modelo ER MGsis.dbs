		<function name="processa_ordem_notafiscal_loop" id="6b51f5e0-29c7-453f-96d8-5333b67a3491" isSystem="false" params_known="n" >
			<string><![CDATA[CREATE OR REPLACE FUNCTION mgsis.processa_ordem_notafiscal_loop()
 RETURNS void
 LANGUAGE plpgsql
AS $function$
DECLARE
    registros_pendentes INTEGER;
    mensagem_log TEXT;
    update_script TEXT;
BEGIN
    SELECT COUNT(*) INTO registros_pendentes
    FROM tblnotafiscalprodutobarra
    WHERE ordem IS NULL;
    RAISE NOTICE 'InÃ­cio do processamento em loop. Registros pendentes iniciais: %', registros_pendentes;
    WHILE registros_pendentes > 0 LOOP
        SET session_replication_role = 'replica';
        mensagem_log := 'Processando lote. Registros pendentes restantes: ' || registros_pendentes;
        RAISE NOTICE '%', mensagem_log;
        update_script := $SQL$
            BEGIN;
            UPDATE tblnotafiscalprodutobarra t
            SET ordem = sub.nova_ordem
            FROM (
                SELECT
                    codnotafiscalprodutobarra,
                    ROW_NUMBER() OVER (
                        PARTITION BY codnotafiscal
                        ORDER BY codnotafiscalprodutobarra
                    ) AS nova_ordem
                FROM tblnotafiscalprodutobarra
                WHERE codnotafiscal IN (
                    SELECT DISTINCT iqnf.codnotafiscal
                    FROM tblnotafiscalprodutobarra iqnf
                    WHERE iqnf.ordem IS NULL
                    LIMIT 1000
                )
            ) AS sub
            WHERE t.codnotafiscalprodutobarra = sub.codnotafiscalprodutobarra;
            COMMIT;
        $SQL$;
        BEGIN
            EXECUTE update_script;
        EXCEPTION
            WHEN OTHERS THEN
                RAISE EXCEPTION 'âŒ Erro no UPDATE/COMMIT do lote: %', SQLERRM;
        END;
        SET session_replication_role = 'origin';
        SELECT COUNT(*) INTO registros_pendentes
        FROM tblnotafiscalprodutobarra
        WHERE ordem IS NULL;
    END LOOP;
    RAISE NOTICE 'âœ… Processamento concluÃ­do. Todos os registros com ordem NULL foram atualizados.';
END; 
$function$
 ;]]></string>
		</function>
		<function name="set_ordem_nf_produto" id="14702683-ce7c-4f1f-ada2-f351f59fe83f" isSystem="false" params_known="n" >
			<string><![CDATA[CREATE OR REPLACE FUNCTION mgsis.set_ordem_nf_produto()
