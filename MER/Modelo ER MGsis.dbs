		<materialized_view name="mwrankingvendasproduto" >
			<view_script><![CDATA[CREATE MATERIALIZED VIEW ${materializedView} AS  SELECT tpb.codproduto,
    count(tnpb.codnegocioprodutobarra) AS vendas,
    dense_rank() OVER (ORDER BY (count(tnpb.codnegocioprodutobarra)) DESC, (min(tnpb.criacao)), tpb.codproduto) AS ranking
   FROM (tblnegocioprodutobarra tnpb
     JOIN tblprodutobarra tpb ON ((tpb.codprodutobarra = tnpb.codprodutobarra)))
  WHERE (tnpb.criacao >= (CURRENT_DATE - '1 year'::interval))
  GROUP BY tpb.codproduto
  ORDER BY (dense_rank() OVER (ORDER BY (count(tnpb.codnegocioprodutobarra)) DESC, (min(tnpb.criacao)), tpb.codproduto))]]></view_script>
