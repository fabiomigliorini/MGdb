		<view name="vwboleto" >
			<view_script><![CDATA[CREATE OR REPLACE VIEW ${view} AS SELECT tblbanco.numerobanco,
    tblportador.agencia,
    tblportador.agenciadigito,
    tbltitulo.nossonumero,
    tblportador.conta,
    tblportador.contadigito,
    tbltitulo.vencimento,
    tbltitulo.saldo,
    round(((tbltitulo.saldo * (2)::numeric) / (100)::numeric), 2) AS multa,
    round((((tbltitulo.saldo * (4)::numeric) / (100)::numeric) / (30)::numeric), 2) AS juros,
    tbltitulo.boleto,
    tbltitulo.codtitulo,
    tbltitulo.emissao,
    tbltitulo.numero,
    tbltitulo.transacao,
    tbltitulo.fatura,
    tblpessoafilial.pessoa AS pessoafilial,
    tblpessoa.pessoa,
    tblpessoa.codpessoa,
    tblpessoa.enderecocobranca,
    tblpessoa.numerocobranca,
    tblpessoa.complementocobranca,
    tblpessoa.bairrocobranca,
    tblpessoa.cepcobranca,
    tblcidade.cidade,
    tblestado.sigla,
    tblportador.convenio,
    tblportador.codportador,
    tblportador.carteira,
    tblpessoa.fisica,
    tblpessoa.cnpj,
    tbltitulo.remessa
   FROM (((((((tbltitulo
     LEFT JOIN tblportador ON ((tbltitulo.codportador = tblportador.codportador)))
     LEFT JOIN tblbanco ON ((tblportador.codbanco = tblbanco.codbanco)))
     LEFT JOIN tblfilial ON ((tblfilial.codfilial = tbltitulo.codfilial)))
     LEFT JOIN tblpessoa tblpessoafilial ON ((tblfilial.codpessoa = tblpessoafilial.codpessoa)))
     LEFT JOIN tblpessoa ON ((tbltitulo.codpessoa = tblpessoa.codpessoa)))
     LEFT JOIN tblcidade ON ((tblcidade.codcidade = tblpessoa.codcidadecobranca)))
     LEFT JOIN tblestado ON ((tblestado.codestado = tblcidade.codestado)))
