		<function name="fntblmovimentotituloaiauad" id="Function4846928" isSystem="false" params_known="n" >
			<string><![CDATA[CREATE OR REPLACE FUNCTION mgsis.fntblmovimentotituloaiauad()
 RETURNS trigger
 LANGUAGE plpgsql
AS $function$
				DECLARE
				vCodTitulo bigint;
				vCodLiquidacaoTitulo bigint;
				vTransacaoLiquidacao date;
				vTransacaoEstorno timestamp without time zone;
				vDebitoTotal numeric (14,2);
				vDebitoSaldo numeric (14,2);
				vCreditoTotal numeric (14,2);
				vCreditoSaldo numeric (14,2);
				vSaldo numeric (14,2);
				BEGIN
				IF (TG_OP = 'DELETE') THEN
					vCodTitulo = OLD.codTitulo;
					vCodLiquidacaoTitulo = OLD.codLiquidacaoTitulo;
				ELSE
					vCodTitulo = NEW.codTitulo;
					vCodLiquidacaoTitulo = NEW.codLiquidacaoTitulo;
				END IF;
				SELECT 
				  INTO vDebitoTotal
					 , vCreditoTotal
					   SUM(COALESCE(tblMovimentoTitulo.Debito, 0))
					 , SUM(COALESCE(tblMovimentoTitulo.Credito, 0))
				FROM tblMovimentoTitulo
				WHERE tblMovimentoTitulo.codTitulo = vCodTitulo;
				vSaldo := COALESCE(vDebitoTotal, 0) - COALESCE(vCreditoTotal, 0);
				vTransacaoLiquidacao := NULL;
				vTransacaoEstorno := NULL;
				IF vSaldo > 0 THEN 
					vDebitoSaldo := vSaldo;
					vCreditoSaldo := 0;
				ELSIF vSaldo < 0 THEN 
					vDebitoSaldo := 0;
					vCreditoSaldo := vSaldo * -1;
				ELSE
					vDebitoSaldo := 0;
					vCreditoSaldo := 0;
					SELECT INTO vTransacaoEstorno MAX(COALESCE(criacao, sistema)) 
					  FROM tblMovimentoTitulo
					 WHERE tblMovimentoTitulo.codTitulo = vCodTitulo
					   AND tblMovimentoTitulo.codTipoMovimentoTitulo = 900;
					SELECT INTO vTransacaoLiquidacao MAX(Transacao) 
					  FROM tblMovimentoTitulo 
					 WHERE tblMovimentoTitulo.codTitulo = vCodTitulo;
				END IF;
				UPDATE tblTitulo
				   SET DebitoTotal         = vDebitoTotal
					 , CreditoTotal        = vCreditoTotal
					 , DebitoSaldo         = vDebitoSaldo
					 , CreditoSaldo        = vCreditoSaldo
					 , Saldo               = vSaldo
					 , TransacaoLiquidacao = vTransacaoLiquidacao
					 , Estornado           = vTransacaoEstorno
				 WHERE tblTitulo.codTitulo = vCodTitulo;
				IF vCodLiquidacaoTitulo	IS NOT NULL THEN
					update tblliquidacaotitulo
					set 
						debito = q.debito,
						credito = q.credito
					from (
						SELECT 
							  tblmovimentotitulo.codliquidacaotitulo
							, sum(coalesce(tblmovimentotitulo.debito, 0)) AS debito
							, sum(coalesce(tblmovimentotitulo.credito, 0)) AS credito
						FROM tblmovimentotitulo
						WHERE tblmovimentotitulo.codliquidacaotitulo = vCodLiquidacaoTitulo
						AND tblmovimentotitulo.codtipomovimentotitulo = 600 
						GROUP BY tblmovimentotitulo.codliquidacaotitulo
						) q
					where tblliquidacaotitulo.codliquidacaotitulo = vCodLiquidacaoTitulo
					and tblliquidacaotitulo.codliquidacaotitulo = q.codliquidacaotitulo;
				END IF;
				RETURN NEW;
				END;
			$function$
 ;]]></string>
		</function>
		<function name="fntblnegocio_atualiza_valoraprazo" id="Function-16de" isSystem="false" params_known="y" >
			<string><![CDATA[CREATE OR REPLACE FUNCTION mgsis.fntblnegocio_atualiza_valoraprazo(pcodnegocio bigint)
 RETURNS boolean
 LANGUAGE plpgsql
AS $function$
			BEGIN
				with totais as (
					SELECT 
						sum(case when COALESCE(tblformapagamento.avista, true) then tblnegocioformapagamento.valorpagamento else 0 end) as valoravista, 
						sum(case when NOT COALESCE(tblformapagamento.avista, false) then tblnegocioformapagamento.valorpagamento else 0 end) as valoraprazo,
						sum(valorjuros) as valorjuros 
					  FROM tblnegocioformapagamento
					 INNER JOIN tblformapagamento ON tblformapagamento.codformapagamento = tblnegocioformapagamento.codformapagamento
					 WHERE tblnegocioformapagamento.CodNegocio = pCodNegocio
				)
				update tblnegocio 
				set valorjuros  = coalesce(totais.valorjuros, 0),
				valoravista = coalesce(totais.valoravista, 0),
				valoraprazo = coalesce(totais.valoraprazo, 0),
				valorTotal  = coalesce(tblNegocio.valorProdutos, 0) - coalesce(tblNegocio.valorDesconto, 0) + coalesce(tblNegocio.valorFrete, 0) + coalesce(totais.valorJuros, 0)
				from totais 
				where tblnegocio.codnegocio = pCodNegocio
				 AND tblNegocio.codpdv is null;
				RETURN TRUE;
			END;
			$function$
;]]></string>
			<input_param name="pcodnegocio" jt="-1" type="bigint" inOut="1" />
		</function>
		<function name="fntblnegocio_atualiza_valorprodutos" id="Function-69" isSystem="false" params_known="y" >
			<string><![CDATA[CREATE OR REPLACE FUNCTION mgsis.fntblnegocio_atualiza_valorprodutos(pcodnegocio bigint)
 RETURNS boolean
 LANGUAGE plpgsql
AS $function$
			DECLARE
				vValorProdutos numeric (14,2);
			BEGIN
				SELECT 
				  INTO vValorProdutos
					   SUM(COALESCE(tblNegocioProdutoBarra.valorProdutos, tblNegocioProdutoBarra.valortotal, 0))
				FROM tblNegocioProdutoBarra
				WHERE tblNegocioProdutoBarra.codNegocio = pCodNegocio
				AND tblNegocioProdutoBarra.inativo is null;
				UPDATE tblNegocio
				   SET valorProdutos       = coalesce(vValorProdutos, 0)
				 WHERE tblNegocio.CodNegocio = pCodNegocio
				 AND tblNegocio.codpdv is null;
				RETURN TRUE;
			END;
			$function$
 ;]]></string>
			<input_param name="pcodnegocio" jt="-1" type="bigint" inOut="1" />
		</function>
		<function name="fntblnegocio_atualiza_valortotal" id="Function-1da" isSystem="false" params_known="y" >
			<string><![CDATA[CREATE OR REPLACE FUNCTION mgsis.fntblnegocio_atualiza_valortotal(pcodnegocio bigint)
 RETURNS boolean
 LANGUAGE plpgsql
AS $function$
			BEGIN
				UPDATE tblNegocio
				   SET valorAVista = 
				   		coalesce(tblNegocio.valorProdutos, 0) 
				   		- coalesce(tblNegocio.valorDesconto, 0) 
				   		- coalesce(tblNegocio.valorAPrazo, 0) 
				   		+ coalesce(tblNegocio.valorFrete, 0) 
					 	+ coalesce(tblNegocio.valorSeguro, 0) 
					 	+ coalesce(tblNegocio.valorOutras, 0) 
				   		+ coalesce(tblNegocio.valorJuros, 0)
					 , valorTotal  = 
					 	coalesce(tblNegocio.valorProdutos, 0) 
					 	- coalesce(tblNegocio.valorDesconto, 0) 
					 	+ coalesce(tblNegocio.valorFrete, 0) 
					 	+ coalesce(tblNegocio.valorSeguro, 0) 
					 	+ coalesce(tblNegocio.valorOutras, 0) 
					 	+ coalesce(tblNegocio.valorJuros, 0)
				 WHERE tblNegocio.CodNegocio = pCodNegocio
				 AND tblNegocio.codpdv is null;
				RETURN TRUE;
			END;
			$function$
 ;]]></string>
			<input_param name="pcodnegocio" jt="-1" type="bigint" inOut="1" />
		</function>
